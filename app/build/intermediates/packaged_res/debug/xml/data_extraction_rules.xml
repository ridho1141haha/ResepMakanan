<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- For more information on what to include and exclude from cloud backup, see
        https://developer.android.com/guide/topics/data/autobackup#IncludeExclude
        -->
        <exclude domain="database" />
        <exclude domain="sharedpref" />
        <exclude domain="file" />
        <exclude domain="root" />
        <exclude domain="external" />
    </cloud-backup>
    <device-transfer>
        <!-- For more information on what to include and exclude from device-to-device
        transfer, see https://developer.android.com/guide/topics/data/d2d-backup#IncludeExclude
        -->
        <exclude domain="database" />
        <exclude domain="sharedpref" />
        <exclude domain="file" />
        <exclude domain="root" />
        <exclude domain="external" />
    </device-transfer>
</data-extraction-rules>